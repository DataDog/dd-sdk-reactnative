{
  "name": "dd-sdk-reactnative",
  "version": "1.0.0-beta2",
  "description": "A client-side React Native module to interact with Datadog",
  "keywords": [
    "datadog",
    "react-native",
    "ios",
    "android"
  ],
  "author": "Datadog (https://github.com/DataDog)",
  "homepage": "https://github.com/DataDog/dd-sdk-reactnative#readme",
  "repository": "https://github.com/DataDog/dd-sdk-reactnative",
  "bugs": {
    "url": "https://github.com/DataDog/dd-sdk-reactnative/issues"
  },
  "license": "Apache-2.0",
  "private": true,
  "main": "lib/commonjs/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "dd-sdk-reactnative.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "module": "lib/module/index",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "prepare": "bob build",
    "example": "yarn --cwd example"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.12.15",
    "@react-navigation/native": "5.9.2",
    "@react-navigation/stack": "5.14.2",
    "@testing-library/react-native": "7.0.2",
    "@types/jest": "26.0.19",
    "@types/react": "^16.13.1",
    "@types/react-native": "0.63.42",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.18.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-native": "3.10.0",
    "jest": "26.6.3",
    "pod-install": "0.1.14",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "0.17.1",
    "react-native-gesture-handler": "1.10.3",
    "react-native-safe-area-context": "3.1.9",
    "react-test-renderer": "16.13.1",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "@react-navigation/native": "^5.6.0",
    "react": "^16.13.1",
    "react-native": "^0.63.4"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": []
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
